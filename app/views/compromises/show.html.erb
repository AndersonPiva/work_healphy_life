<div class="skin-blue sidebar-mini new-height">
  <div class="wrapper">
    <header class="main-header">
      <%= render "privates/navbarprivate" %>
    </header>
    <aside class="main-sidebar">
      <%= render "privates/menubar" %>
    </aside>
    <div class="content-wrapper">
      <div class="perfil-content">
        <div class="panel panel-default col-xs-10 col-xs-offset-1">
          <h1 class="col-xs-offset-1">
            <i class="fa fa-plus-circle"></i>
            Detalhes do Compromisso</h1>
          <h4 class="col-xs-offset-1">Veja todos os detalhes do compromisso para organizar-se com antecedência.</h4>
          <div class="div-content">
            <div class="panel panel-default col-xs-10 col-xs-offset-1">
              <div class="form-horizontal">
                <legend>
                  <i class="fa fa-tags"></i>
                  Dados Gerais
                </legend>
                <div class="form-group field">
                  <label class="col-sm-2 control-label" for="nome">Titulo</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" value="<%= @compromise.title%>" disabled/>
                  </div>
                </div>
                <div class="form-group field">
                  <label class="col-sm-2 control-label" for="telefone1">Data</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" value="<%= @compromise.date%>" disabled/>
                  </div>
                </div>
                <div class="form-group field">
                  <label class="col-sm-2 control-label" for="telefone1">Hora</label>
                  <div class="col-sm-9">
                    <input type="text" class="form-control" value="<%= @compromise.schedule%>" disabled/>
                  </div>
                </div>
                <div class="form-group field">
                  <label class="col-sm-2 control-label" for="telefone2">Descrição</label>
                  <div class="col-sm-9">
                    <textarea class="form-control" disabled><%= @compromise.description%></textarea>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%= render "privates/footerprivate" %>
</div>
