<%= simple_form_for(@rating) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :muscleWeight %>
    <%= f.input :residualWeigth %>
    <%= f.input :boneWeight %>
    <%= f.input :bodyFat %>
    <%= f.input :excessWeigth %>
    <%= f.input :imc %>
    <%= f.association :patient %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
