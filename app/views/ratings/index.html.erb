<p id="notice"><%= notice %></p>

<h1>Listing Ratings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Muscleweight</th>
      <th>Residualweigth</th>
      <th>Boneweight</th>
      <th>Bodyfat</th>
      <th>Excessweigth</th>
      <th>Imc</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ratings.each do |rating| %>
      <tr>
        <td><%= rating.date %></td>
        <td><%= rating.muscleWeight %></td>
        <td><%= rating.residualWeigth %></td>
        <td><%= rating.boneWeight %></td>
        <td><%= rating.bodyFat %></td>
        <td><%= rating.excessWeigth %></td>
        <td><%= rating.imc %></td>
        <td><%= rating.patient %></td>
        <td><%= link_to 'Show', rating_path(rating, format: :pdf) %></td>
        <td><%= link_to 'Edit', edit_rating_path(rating) %></td>
        <td><%= link_to 'Destroy', rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating', new_rating_path %>
